import textwrap

def wrap(string, max_width):
    sub_leng = int(len(string)/max_width)
    result = ""
    for count in range (sub_leng+1):
        ini_leng = max_width * count
        end_leng = max_width * (count + 1)
        tmp = result
        result = tmp + string[ini_leng:end_leng] + "\n"
    return result

if __name__ == '__main__':
    string, max_width = input(), int(input())
    result = wrap(string, max_width)
    print(result)
