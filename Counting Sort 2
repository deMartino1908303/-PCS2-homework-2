def countingSort_list(arr):
    lis = [0] * 100
    for value in arr:
        lis[value] += 1
    return lis

    
def countingSort(arr):
    
    counted_arr = countingSort_list(arr)
    sorted_lis = []
    index_counter = 0
    
    for counter in counted_arr:
        
        for times in range(counter):
            
            sorted_lis.append(index_counter)
        
        index_counter += 1
    
    return sorted_lis
